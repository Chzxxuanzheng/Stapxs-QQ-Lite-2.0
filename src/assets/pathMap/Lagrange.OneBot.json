{
    "_name": "Lagrange.OneBot",
    "version_info": "$.data",
    "login_info": {
        "uin": "$.data.user_id",
        "bkn": "",
        "nickname": "$.data.nickname"
    },
    "user_list": {
        "_basic": "$.data[*]",
        "_list": {
            "group_id": "/group_id",
            "group_name": "/group_name",
            "member_count": "/member_count",
            "admin_flag": "$.admins[?(@ == '<uin>')]",

            "class_id": "/group_id",
            "class_name": "/group_name",
            "user_id": "/user_id",            
            "nickname": "/nickname",
            "remark": "/remark"
        }
    },
    "roaming_stamp": {
        "_name": "fetch_custom_face"
    },
    "message_list": {
        "_name": "get_group_msg_history",
        "_basic": "$.data.messages[*]",
        "_type": "json_with_data|$.data",
        "_message_type": {
            "user": "private",
            "group": "group"
        },
        "_list": {
            "time": "/time",
            "message_type": "/message_type",
            "post_type": "",
            "message_id": "/message_id",
            "group_id": "/group_id",
            "sender": "/sender",
            "message": "/message",
            "raw_message": "/raw_message"
        }
    },
    "message_list_private": {
        "_name": "get_friend_msg_history"
    },
    "forward_msg": {
        "_name": "get_forward_msg",
        "_basic": "$.data.message[*].data"
    },
    "message_info": {
        "message_id": "$.message_id",
        "private_id": "$.user_id",
        "group_id": "$.group_id",
        "sender": "$.sender.user_id"
    },
    "get_message": "$.message",
    "message_value": {
        "image": { "url": "$.file" }
    }
}